            "attack": {
                "Kamisato_Art_Naname": {
                    "name": "Kamisato Art - Naname",
                    "effect": "Normal Attack\nPerform up to 5 rapid strikes.\nCharged Attack\nConsumes a certain amount of Stamina to unleash a continuous stream of sword ki.\nPlunging Attack\nPlunges from mid-air to strike the ground below, damaging enemies along the path and dealing AoE DMG upon impact.",
                    "image": "",
                    "order": 0,
                    "type": "Normal/Charged Attack"
                },
                "Kamisato_Art_Hyouka": {
                    "name": "Kamisato Art: Hyouka",
